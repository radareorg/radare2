#!/bin/sh
# GPL 3+ - Copyright (C) 2011-2012  pancake, Edd Barrett, Simon Ruderich
[ -e tests.sh ] && . ./tests.sh || . ../tests.sh

NAME='oi-'
FILE=malloc://1024
ARGS=
#CMDS='o-`o~[1]`;o'
CMDS='oi-'
EXPECT=
run_test

NAME='o-'
FILE=malloc://1024
ARGS=
CMDS='o-`o~[2]`;o'
EXPECT=
run_test

# 3, because one for the adress returned by o -,
# and the two other are the opened files
NAME='o -'
FILE=malloc://1024
ARGS=
CMDS='e file.nowarn=true
e file.loadmethod=append
o -;o~?'
EXPECT='3
'
run_test

NAME='om'
FILE=malloc://1024
ARGS=
CMDS='om `o~[1]` 0x4000
om~?'
EXPECT='2
'
run_test

NAME='om2'
FILE=malloc://1024
ARGS=
CMDS='om `o~[1]` 0x4000
om `o~[1]` 0x4000
om~?'
EXPECT='2
'
run_test

NAME='oob 10'
FILE=../bins/elf/analysis/hello-linux-x86_64
ARGS=
CMDS='10oob
pi 1
'
EXPECT='xor ebp, ebp
'
IGNORE_ERR=1
run_test

NAME='oo+ 10'
FILE=../bins/elf/analysis/hello-linux-x86_64
ARGS=
CMDS='10oo+
pi 1
'
EXPECT='xor ebp, ebp
'
IGNORE_ERR=1
run_test

NAME='oob consider baddr'
FILE=../bins/mach0/mac-ls
ARGS=
CMDS='e bin.baddr = 0xf00000
k old_v = `ieq`
oob
ie~baddr[5]
ie~vaddr[1]
p8 10 @ entry0
p8 10 @ `k old_v`
'
EXPECT='0x00f00000
0x00f01058
554889e5415741564155
ffffffffffffffffffff
'
run_test

NAME='oob consider laddr'
FILE=../bins/mach0/fatmach0-3true
ARGS=
CMDS='e bin.laddr = 0x5000
e bin.baddr = -1
oob
ie~laddr[7]
ie~baddr[5]
'
EXPECT='0x00005000
0x00001000
'
run_test

NAME='oob from malloc'
BROKEN=1
FILE=malloc://1024
ARGS=
CMDS='wx 
i~bintype[1]
p8 10 @ entry0
'
EXPECT='elf
b32ae931000000000200
'
run_test

NAME='oob bin.baddr = 0'
FILE=../bins/elf/analysis/main
ARGS=-n
CMDS='e bin.baddr = 0
oob
iS~.comment
'
EXPECT='idx=25 vaddr=0x00000000 paddr=0x00000910 sz=34 vsz=34 perm=----- name=.comment
'

NAME='ob select files'
FILE=malloc://1024
ARGS=
CMDS='
e scr.null=true
o malloc://512
e scr.null=false
i~file[1]
obb `ob~:0[2]`
i~file[1]
obo `ob~:3[1]`
i~file[1]
ob `ob~:0[2]` `ob~:1[1]`
i~file[1]
'
EXPECT='malloc://512
malloc://1024
malloc://512
malloc://1024
'
run_test

NAME='ob select files binobj'
FILE=../bins/elf/libmagic.so
ARGS=
CMDS='
iiq~?
e scr.null=true
o malloc://1024
e scr.null=false
iiq~?
'
EXPECT='38
0
'
run_test

NAME='ob select files binobj'
FILE=../bins/elf/libmagic.so
ARGS=
CMDS='
isq~?
e scr.null=true
o ../bins/elf/true32
e scr.null=false
isq~?
# raise back
o `o~:0[2]`
isq~?
'
EXPECT='408
46
408
'
run_test
