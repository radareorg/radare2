#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME='rahash2 -a md5'
CMDS='!rahash2 -a md5 ../../bins/elf/analysis/hello-linux-x86_64'
BROKEN=
EXPECT='../../bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a35 md5: c957bd5bd6204470256bc15248ccafd4
'

run_test

NAME='rahash2 -a sha1'
CMDS='!rahash2 -a sha1 ../../bins/elf/analysis/hello-linux-x86_64'
BROKEN=
EXPECT='../../bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a35 sha1: 687c82d13cb27f0600d8e57edc784282c1732f56
'

run_test

NAME='rahash2 -a sha256'
CMDS='!rahash2 -a sha256 ../../bins/elf/analysis/hello-linux-x86_64'
BROKEN=
EXPECT='../../bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a35 sha256: 7bdbf25324af1946ec0b16dbf928875a588a786f7c279cd115729c5a3a297a55
'

run_test

NAME='rahash2 -a sha384'
CMDS='!rahash2 -a sha384 ../../bins/elf/analysis/hello-linux-x86_64'
EXPECT='../../bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a35 sha384: a6fed224b0b5892ab44839b5ae6a12e54fb3393b5f1e1ca174fb5a0032c994f1c492c5b3a090518d8a60875223977ba4
'

run_test

NAME='rahash2 -a sha512'
CMDS='!rahash2 -a sha512 ../../bins/elf/analysis/hello-linux-x86_64'
EXPECT='../../bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a35 sha512: 2640b1ff96870fb78a3d8ae6b51595ca86c37e25443b2e8c5441e77d11cdb3830ece8d791561c22788d37d7f22c99cdabf8f798826c0ff441901e6157890fee6
'

run_test

NAME='rahash2 -a md5,sha1,sha256 -r'
CMDS='!rahash2 -a md5,sha1,sha256 -r ../../bins/elf/analysis/hello-linux-x86_64'
BROKEN=
EXPECT='../../bins/elf/analysis/hello-linux-x86_64: e file.md5=c957bd5bd6204470256bc15248ccafd4
../../bins/elf/analysis/hello-linux-x86_64: e file.sha1=687c82d13cb27f0600d8e57edc784282c1732f56
../../bins/elf/analysis/hello-linux-x86_64: e file.sha256=7bdbf25324af1946ec0b16dbf928875a588a786f7c279cd115729c5a3a297a55
'

run_test

NAME='rahash2 -b10 -a md5,sha1,sha256 -r'
CMDS='!rahash2 -b10 -a md5,sha1,sha256 -r ../../bins/elf/analysis/hello-linux-x86_64'
BROKEN=
EXPECT='../../bins/elf/analysis/hello-linux-x86_64: e file.md5=c957bd5bd6204470256bc15248ccafd4
../../bins/elf/analysis/hello-linux-x86_64: e file.sha1=687c82d13cb27f0600d8e57edc784282c1732f56
../../bins/elf/analysis/hello-linux-x86_64: e file.sha256=7bdbf25324af1946ec0b16dbf928875a588a786f7c279cd115729c5a3a297a55
'

run_test


NAME='rahash2 -h'
CMDS='!rahash2~Usage'
BROKEN=
EXPECT='Usage: rahash2 [-rBhLkv] [-b S] [-a A] [-c H] [-E A] [-s S] [-f O] [-t O] [file] ...
'

run_test

NAME='rahash2 -a md5 -s "admin"'
CMDS='!rahash2 -a md5 -s "admin"'
BROKEN=
EXPECT='0x00000000-0x00000004 md5: 21232f297a57a5a743894a0e4a801fc3
'

run_test

NAME='rahash2 -a md5 -e -s "admin"'
CMDS='!rahash2 -a md5 -e -s "admin"'
BROKEN=
EXPECT='0x00000000-0x00000004 md5: c31f804a0e4a8943a7a5577a292f2321
'

run_test


NAME='rahash2 -a sha1,sha256 -s "admin"'
CMDS='!rahash2 -a sha1,sha256 -s "admin"'
BROKEN=
EXPECT='0x00000000-0x00000004 sha1: d033e22ae348aeb5660fc2140aec35850c4da997
0x00000000-0x00000004 sha256: 8c6976e5b5410415bde908bd4dee15dfb167a9c873fc4bb8a81f6f2ab448a918
'

run_test


NAME='rahash2 -D base64 -s "YWRtaW4="'
CMDS='!rahash2 -D base64 -s "YWRtaW4="'
BROKEN=
EXPECT='admin'

run_test

NAME='rahash2 -E base64 -s "admin"'
CMDS='!rahash2 -E base64 -s "admin"'
BROKEN=
EXPECT='YWRtaW4='

run_test

NAME='rahash2 -E base64 -x "61646d696e"'
CMDS='!rahash2 -E base64 -x "61646d696e"'
BROKEN=
EXPECT='YWRtaW4='

run_test

NAME='rahash2 -a md5 -x "61646d696e"'
CMDS='!rahash2 -a md5 -x "61646d696e"'
BROKEN=
EXPECT='0x00000000-0x00000004 md5: 21232f297a57a5a743894a0e4a801fc3
'

run_test

NAME='rahash2 -a md5 -x "61646d696e" -s "admin"'
CMDS='!rahash2 -a md5 -x "61646d696e" -s "admin"'
BROKEN=
EXPECT_ERR='Hashstring already defined
'
run_test

NAME='rahash2 -a luhn -s "5105105105105100"'
CMDS='!rahash2 -a luhn -s "5105105105105100"'
BROKEN=
EXPECT='0x00000000-0x0000000f luhn: 00
'

run_test

NAME='rahash2 -a luhn -s "4408041234567890"'
CMDS='!rahash2 -a luhn -s "4408041234567890"'
BROKEN=
EXPECT='0x00000000-0x0000000f luhn: 07
'
run_test

NAME='rahash2 -L'
CMDS='!rahash2 -L'
BROKEN=
EXPECT='  md5
  sha1
  sha256
  sha384
  sha512
  crc16
  crc32
  md4
  xor
  xorpair
  parity
  entropy
  hamdist
  pcprint
  mod255
  xxhash
  adler32
  luhn
  rc2
  rc4
  rc6
  aes-ecb
  aes-cbc
  ror
  rol
  rot
  blowfish
  cps2
  des-ecb
'

run_test

NAME='rahash2 -a md5 -t0x4 -x "61646d696eaa"'
CMDS='!rahash2 -a md5 -t0x4 -x "61646d696eaa"'
BROKEN=
