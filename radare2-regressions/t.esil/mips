#!/bin/sh
for a in . .. ../.. ; do [ -e $a/tests.sh ] && . $a/tests.sh ; done

NAME="add zero, zero, 5"
FILE=malloc://0x200
CMDS='
e asm.arch=mips
e asm.bits=32
ar > /dev/null
ar v0=5
wx 20000200
aes
ar zero
'
EXPECT='0x00000000
'
run_test

NAME="add a1, zero, 5"
FILE=malloc://0x200
CMDS='
e asm.arch=mips
e asm.bits=32
ar > /dev/null
ar v0=5
wx 20280200
aes
ar a1
'
EXPECT='0x00000005
'
run_test

NAME="add a1, 0xFFFFFFFF, 5"
FILE=malloc://0x200
CMDS='
