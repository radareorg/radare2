NAME=serial relsub
FILE=../bins/elf/analysis/serial
EXPECT=<<EOF
  0x00400b35      lea rax, [0x0040073d]
  0x00400b35      lea rax, [fcn.0040073d]
EOF
CMDS=<<EOF
s 0x00400b35
e asm.leahints=false
e asm.bytes=false
e asm.lines.bb=false
e asm.relsub=true
e asm.comments=false
pd 1
af @ 0x0040073d
pd 1
EOF
RUN
