NAME=VSF: Rocky Horror Picture Show- open
FILE=../bins/vsf/c128-rhps-rom.vsf
ARGS=-A
EXPECT=<<EOF
EOF
CMDS=<<EOF
q!
EOF
RUN

NAME=VSF: Rocky Horror Picture Show- detection
FILE=../bins/vsf/c128-rhps-rom.vsf
ARGS=
EXPECT=<<EOF
os       C128
EOF
CMDS=<<EOF
iI~os
EOF
RUN

