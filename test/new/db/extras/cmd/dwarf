NAME="dwarf: idd and idda"
FILE=../../bins/elf/bomb
CMDS=<<EXPECT
e io.cache = 1
iddi ../../bins/elf/bomb
wx 900d400000000000b00d400000000000
idd _IO_marker
idd _IO_marker._pos
idda _IO_marker._sbuf._IO_read_base
EXPECT=<<RUN
_next = 0x400d90,
_sbuf = 0x400db0,
_pos = 0x22a0c0c7
addr = 0x400ca0
addr = 0x400dc8
RUN

NAME="dwarf: iddv"
FILE=../../bins/elf/bomb
CMDS=<<EXPECT
e io.cache = 1
iddi ../../bins/elf/bomb
wx 900d400000000000b00d400000000000
iddv _IO_marker._sbuf._flags
EXPECT=<<RUN
0x2029b4
RUN

NAME="dwarf: iddlf"
CMDS=<<EXPECT
iddi ../../bins/elf/bomb
iddlf
EXPECT=<<RUN
f sym.main 311 @ 0x400da0
RUN

NAME="dwarf: iddlg"
CMDS=<<EXPECT
iddi ../../bins/elf/analysis/pie
iddlg
EXPECT=<<RUN
f sym._IO_stdin_used 4 @ 0x65c
RUN

NAME="dwarf: iddd -- without array"
CMDS=<<EXPECT
iddi ../../bins/elf/bomb
iddd _IO_marker
EXPECT=<<RUN
struct _IO_marker {
  struct _IO_marker *_next;
  struct _IO_FILE *_sbuf;
  int _pos;
};
RUN

NAME="dwarf: iddt"
CMDS=<<EXPECT
iddi ../../bins/elf/bomb
iddt _IO_marker._sbuf
iddt _IO_marker._pos
EXPECT=<<RUN
type : struct _IO_FILE *
size : 8
type : int 
size : 4
RUN

NAME="dwarf: iddd -- array"
CMDS=<<EXPECT
iddi ../../bins/elf/ezpz
iddd _IO_FILE
EXPECT=<<RUN
struct _IO_FILE {
  int _flags;
  char *_IO_read_ptr;
  char *_IO_read_end;
  char *_IO_read_base;
  char *_IO_write_base;
  char *_IO_write_ptr;
  char *_IO_write_end;
  char *_IO_buf_base;
  char *_IO_buf_end;
  char *_IO_save_base;
  char *_IO_backup_base;
  char *_IO_save_end;
  struct _IO_marker *_markers;
  struct _IO_FILE *_chain;
  int _fileno;
  int _flags2;
  __off_t _old_offset;
  short unsigned int _cur_column;
  signed char _vtable_offset;
  char _shortbuf[1];
  _IO_lock_t *_lock;
  __off64_t _offset;
  void *__pad1;
  void *__pad2;
  void *__pad3;
  void *__pad4;
  size_t __pad5;
  int _mode;
  char _unused2[20];
};
RUN

