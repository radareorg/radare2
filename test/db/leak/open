NAME=leak opening malloc
FILE=malloc://32
CMDS=<<EOF
?e hello
EOF
EXPECT=<<EOF
hello
EOF
RUN

NAME=leak opening elf
FILE=bins/elf/ip-riscv
CMDS=<<EOF
?e hello
EOF
EXPECT=<<EOF
hello
EOF
RUN

NAME=leak opening macho
FILE=bins/mach0/hello-osx-i386
CMDS=<<EOF
?e hello
EOF
EXPECT=<<EOF
hello
EOF
RUN

NAME=leak analysis elf
FILE=bins/elf/ip-riscv
ARGS=-AA
CMDS=<<EOF
?e hello
EOF
EXPECT=<<EOF
hello
EOF
RUN

NAME=leak analysis macho
FILE=bins/mach0/hello-osx-i386
BROKEN=1
ARGS=-AA
CMDS=<<EOF
?e hello
EOF
EXPECT=<<EOF
hello
EOF
RUN
