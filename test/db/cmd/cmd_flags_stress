NAME=no flags
FILE=-
CMDS=<<EOF
f patata
f-*
f~?
EOF
EXPECT=<<EOF
0
EOF
RUN

NAME=no flags
FILE=bins/elf/analysis/elf-relro
CMDS=<<EOF
f patata
fs *
f-*
f~?
EOF
EXPECT=<<EOF
0
EOF
RUN

NAME=fr foo bar
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=64
f foo
fr foo bar
f
pd 1
EOF
EXPECT=<<EOF
0x00000000 1 bar
            ;-- bar:
            0x00000000      0000           add byte [rax], al
EOF
RUN
