NAME=a8
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=64
a8 c745f400000000~type
EOF
EXPECT=<<EOF
type: mov
EOF
RUN

NAME=aom-6502.cs
FILE=-
CMDS=<<EOF
e asm.arch=6502.cs
e asm.bits=8
aom
aoml~?
EOF
EXPECT=<<EOF
brk
96
EOF
RUN

