NAME=a8
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=64
a8 c745f400000000~type
EOF
EXPECT=<<EOF
type: mov
EOF
RUN

NAME=aof
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=64
wx 55
aof rsp
aof rbp
EOF
EXPECT=<<EOF
8,rsp,-=

EOF
RUN

NAME=ao_aoj
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=64
wx c745f400000000
ao
aoj~{}
EOF
EXPECT=<<EOF
