NAME=p2 hello-linux-x86_64
FILE=bins/elf/analysis/hello-linux-x86_64
CMDS=p2 16
EXPECT=<<EOF
----..==----##..
--==####..####..
==..##..------==
--==####..####--
==..==##--##==##
..----####--====
==..==..########
##..##==--==##--
EOF
RUN

NAME=p= 0xff 0x00
FILE=-
BROKEN=1
CMDS=<<EOF
b 2
wx 0xff
p= 2@0
EOF
EXPECT=<<EOF
         ___________________________________________________
00 00ff | __________________________________________________|
01 0000 ||
EOF
RUN

NAME=p=b 0xff 0x00
FILE=-
CMDS=<<EOF
b 2
wx 0xff
e zoom.in=raw
p=b 2 2@0
EOF
EXPECT=<<EOF
0x00000000 000 00ff |###################################################
0x00000001 001 0000 |#
EOF
RUN

NAME=p= - crash
FILE=-
CMDS=<<EOF
e zoom.in = block
p=p" 1 " 1 EN
EOF
EXPECT=<<EOF
0x00000000 000 0000 |#
EOF
RUN

NAME=p=d
FILE=bins/elf/arm1.bin
ARGS=-n
CMDS=p=d 10 @ 0
EXPECT=<<EOF
min:              0  0x0
max:              127  0x7f
unique (count):   6  0x6
range (max-min):  127  0x7f
size (of block):  10  0xa
EOF
RUN

