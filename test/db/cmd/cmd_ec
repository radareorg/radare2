NAME=ec* and attributes
FILE=-
CMDS=<<EOF
eco bright
ec*~pop
eco white2
ec*~bin
ec*~cjmp
ec*~comment
ec*~fname
EOF
EXPECT=<<EOF
ec pop rgb:ff00ff . bold
ec bin rgb:4f1900 . bold
ec cjmp rgb:376b4c . bold underline
ec comment rgb:001296 . italic
ec fname rgb:8d0004 . bold italic
EOF
RUN

NAME=ecHi
FILE=-
CMDS=ecHi xxxx
EXPECT_ERR=<<EOF
Unknown color xxxx
EOF
RUN
