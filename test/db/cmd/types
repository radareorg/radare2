NAME=td anonymous struct in typedef
FILE=--
CMDS=<<EOF
'td struct Foo {int a;};
'td struct Bar { char *hello; int world[]; };
ts~Foo,Bar
ts Foo
ts Bar
EOF
EXPECT=<<EOF
Bar
Foo
pf d a
pf zd hello world
EOF
RUN

NAME=ts add and delete
FILE=--
CMDS=<<EOF
ts-NSString
'td typedef struct {int a;} Foo;
ts
ts Foo
ts-Foo
?e --
ts~Foo
EOF
EXPECT=<<EOF
Foo
pf d a
--
EOF
RUN

NAME=typelinks
FILE=-
CMDS=<<EOF
