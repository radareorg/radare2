NAME=Backward param type propgation
FILE=bins/elf/back1
CMDS=<<EOF
aa
s main
aaft
afv
EOF
EXPECT=<<EOF
var char * src @ rbp-0x8
var char * dest @ rbp-0x10
var size_t size @ rbp-0x14
EOF
RUN

