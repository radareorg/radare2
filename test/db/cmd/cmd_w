NAME=wt
FILE=-
CMDS=<<EOF
wx 01020304
wt .r2-wt.test 4
p8 4 @ 4
wff .r2-wt.test @ 4
rm .r2-wt.test
p8 4 @ 4
EOF
EXPECT=<<EOF
00000000
01020304
EOF
RUN

NAME=wtf base64
CMDS=<<EOF
wx 01020304
wtf base64:dGVzdGZpbGU== 4
wx 00000000
wff testfile
p8 4
rm testfile
EOF
EXPECT=<<EOF
01020304
EOF
EXPECT_ERR=<<EOF
INFO: Dumped 4 bytes from 0x00000000 into testfile
EOF
RUN

NAME=wtf base64 with alias
CMDS=<<EOF
wx 01020304
wtf base64:JGQ= 4
wx 00000000
wff $d
p8 4
EOF
EXPECT=<<EOF
01020304
EOF
EXPECT_ERR=<<EOF
INFO: Dumped 4 bytes from 0x00000000 into $d
EOF
RUN

