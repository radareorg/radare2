NAME=wp
FILE=-
CMDS=<<EOF
echo 0x10 "hello" > .r2-wp.test
echo 0x20 01020304 >> .r2-wp.test
cat .r2-wp.test
wp .r2-wp.test
rm .r2-wp.test
ps @ 0x10
p8 3 @ 0x20
EOF
EXPECT=<<EOF
0x10 "hello"
0x20 01020304
hello
010203
EOF
RUN

