NAME=pfj z overflow
FILE=malloc://3200
ARGS=-m 1000
CMDS=<<EOF
e io.unalloc=true
s 3046
w AB\x00
pfj [2]z
pfj z
pfj [5]z
?e ----
w AB\xff
pfj [2]z
pfj z
pfj [5]z
pfj z @ 0xfffff
e io.unalloc=false
pfj z @ 0xfffff
EOF
EXPECT=<<EOF
[{"type":"z","offset":3046,"value":"AB"}]
[{"type":"z","offset":3046,"value":"AB"}]
[{"type":"z","offset":3046,"value":"AB"}]
----
[{"type":"z","offset":3046,"value":"AB"}]
