wv4 0x23 @ 0x54
pf *D*D @ 0x50
EOF
EXPECT=<<EOF
(*0x22)push ebp
(*0x23)pop eax
EOF
RUN

NAME=pf *z (64bit addr)
FILE=-
CMDS=<<EOF
e io.cache=true
e asm.bits=64
wz root @ 0x0000000100000faf
pf z @ 0x0000000100000faf
e cfg.bigendian=false
16wx+cc @ 0x200
wv8 0x0000000100000faf @ 0x204
pf *z @ 0x204
e cfg.bigendian=true
16wx+cc @ 0x200
wv8 0x0000000100000faf @ 0x204
pf *z @ 0x204
EOF
EXPECT=<<EOF
0x100000faf = "root"
(*0x100000faf)0x00000204 = "root"
(*0x100000faf)0x00000204 = "root"
EOF
RUN

