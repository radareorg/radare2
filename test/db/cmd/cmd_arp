NAME=bad regprofile
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=64
arps
arp scripts/badrp.r2
arps
ar=
ar rax
EOF
EXPECT=<<EOF
160
88
    rax 0x00000000       
0x00000000
EOF
EXPECT_ERR=<<EOF
Warning: =A0 not defined
EOF
RUN

NAME=bad regprofile 2
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=64
arps
arp scripts/badrp2.r2
arps
ar=
ar rax
EOF
EXPECT=<<EOF
160
1
EOF
EXPECT_ERR=<<EOF
r_reg_set_profile_string: Parse error @ line 3 (Invalid syntax: Wrong number of columns)
EOF
RUN

NAME=bad regprofile 2
FILE=-
