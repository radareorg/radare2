NAME=range
FILE=malloc://1024
CMDS=<<EOF
i~:0
i~:1
i~:2
i~:..3
i~:1..2
i~:-2
i~:-1
?e 0\n1\n2\n3\n4~:-2
?e 0\n1\n2\n3\n4~:-2..
?e 0\n1\n2~:0..
?e 0\n1\n2~:..
EOF
EXPECT=<<EOF
fd       3
file     malloc://1024
size     0x400
fd       3
file     malloc://1024
size     0x400
file     malloc://1024
iorw     true
block    0x100
3
3
4
0
1
2
0
1
2
EOF
RUN

