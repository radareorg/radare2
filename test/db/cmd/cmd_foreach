NAME=foreach iterator
FILE=bins/elf/ls
CMDS=aa ; fd @@@F
EXPECT=<<EOF
entry0
sym._obstack_begin
sym._obstack_begin_1
sym._obstack_allocated_p
sym._obstack_memory_used
sym._obstack_free
sym._obstack_newchunk
main
entry.init0
entry.fini0
fcn.00005b10
EOF
RUN

