NAME=pij asm.flags.middle=0
FILE=malloc://1024
CMDS=<<EOF
e asm.bits=64
e asm.arch = x86
e anal.arch = x86
wx 554889e54157415641554154534881ec
e asm.flags.middle=0
f dat.sym = 2
pij 4~{}
EOF
EXPECT=<<EOF
[
  {
    "offset": 0,
    "esil": "rbp,8,rsp,-,=[8],8,rsp,-=",
    "refptr": 0,
    "fcn_addr": 0,
    "fcn_last": 0,
    "size": 1,
    "opcode": "push rbp",
    "disasm": "push rbp",
    "bytes": "55",
