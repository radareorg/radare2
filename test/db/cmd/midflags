    "offset": 1,
    "esil": "rsp,rbp,=",
    "refptr": 0,
    "fcn_addr": 0,
    "fcn_last": 0,
    "size": 3,
    "opcode": "mov rbp, rsp",
    "disasm": "mov rbp, rsp",
    "bytes": "4889e5",
    "family": "cpu",
    "type": "mov",
    "reloc": false,
    "type_num": 9,
    "type2_num": 0
  },
  {
    "offset": 4,
    "esil": "r15,8,rsp,-,=[8],8,rsp,-=",
    "refptr": 0,
    "fcn_addr": 0,
    "fcn_last": 0,
    "size": 2,
    "opcode": "push r15",
    "disasm": "push r15",
    "bytes": "4157",
    "family": "cpu",
    "type": "rpush",
    "reloc": false,
    "type_num": 268435468,
    "type2_num": 0
  },
  {
    "offset": 6,
    "esil": "r14,8,rsp,-,=[8],8,rsp,-=",
