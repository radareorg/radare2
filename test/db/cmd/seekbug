NAME=seekbug pi -x
FILE=malloc://1024
CMDS=<<EOF
e scr.null=true
s 0x200
pi -3
e scr.null=false
s
EOF
EXPECT=<<EOF
0x200
EOF
RUN

NAME=seekbug pid -x
FILE=malloc://1024
CMDS=<<EOF
e scr.null=true
s 0x200
pid -3
e scr.null=false
s
EOF
EXPECT=<<EOF
0x200
EOF
RUN

