EOF
RUN

NAME=seekbug s ]
FILE=malloc://1024
CMDS=<<EOF
s 0x64
s
s unkval
s
s ]
s
s [
s
s [0x0]
s
EOF
EXPECT=<<EOF
0x64
0x64
0x64
0x64
0x0
EOF
RUN
