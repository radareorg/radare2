NAME=Reopening project with changes loading
FILE=bins/elf/analysis/main
CMDS=<<EOF
e prj.git=false
aaa > /dev/null
e dir.projects = .tmp/
s main > /dev/null
CC test
Ps lapa > /dev/null
Po lapa
pd 1 ~test
rm .tmp/
EOF
EXPECT=<<EOF
|           0x00400506      55             push rbp                    ; test
EOF
RUN
