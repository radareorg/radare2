NAME=Save project with maddr
FILE=bins/elf/analysis/main
ARGS=-n -m 0x1337
CMDS=<<EOF
e prj.git=false
e dir.projects = .tmp/
o
om
om*
?e --
Pd hond
Ps hond > /dev/null
Po hond > /dev/null
om*
?e --
om*~?
om*~[2]
EOF
EXPECT=<<EOF
 3 * r-x 0x000021da bins/elf/analysis/main
 1 fd: 3 +0x00000000 0x00001337 - 0x00003510 r-x 
om 3 0x00001337 0x000021da 0x00000000 r-x
--
om 3 0x00001337 0x000021da 0x00000000 r-x
--
1
0x00001337
EOF
RUN

