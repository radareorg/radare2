NAME=$s (malloc)
FILE=malloc://1024
CMDS=?v $s
EXPECT=<<EOF
0x400
EOF
RUN

NAME=$j (malloc)
FILE=malloc://1024
CMDS=<<EOF
e asm.bits=32
e asm.arch = x86
e anal.arch = x86
wa jmp 0x30
?v $j
wa jz 0x01
?v $j
wa xor eax, eax
?v $j
?e
EOF
EXPECT=<<EOF
0x30
0x1
0xffffffffffffffff

EOF
RUN

NAME=$f (malloc)
FILE=malloc://1024
CMDS=<<EOF
e asm.bits=32
e asm.arch = x86
e anal.arch = x86
wx eb2e
?v $f
wx 0f84fbffffff
?v $f
wx 31c0
?v $f
EOF
EXPECT=<<EOF
0xffffffffffffffff
0x6
0xffffffffffffffff
EOF
RUN

NAME=$m (malloc)
FILE=malloc://1024
CMDS=<<EOF
e asm.arch=x86
e asm.bits=32
wa mov eax, [0x500]
?v $m
wa mov eax, ebx
?v $m
?e
EOF
EXPECT=<<EOF
0x500
0xffffffffffffffff

EOF
RUN

NAME=$l (malloc)
FILE=malloc://1024
CMDS=<<EOF
e asm.bits=32
e asm.arch = x86
e anal.arch = x86
wa xor eax, eax
?v $l
wa mov esp, 0x1
?v $l
wx c3
?v $l
EOF
EXPECT=<<EOF
0x2
0x5
0x1
EOF
RUN

NAME=$e (malloc)
FILE=-
CMDS=<<EOF
e asm.arch=x86
e asm.bits=32
wa inc eax
?v $e
wa jmp 0x01
?v $e
wa ret
?v $e
wa call 0x01
?v $e
EOF
EXPECT=<<EOF
0x0
0x1
0x1
0x0
EOF
RUN

NAME=$e (malloc) 2
FILE=malloc://1024
CMDS=<<EOF
e asm.bits = 0x20
?v ${asm.bits}
EOF
EXPECT=<<EOF
0x20
EOF
RUN

NAME=$e (malloc) 3
FILE=malloc://1024
CMDS=<<EOF
? 5+3
?v $?
? 0
?v $?
EOF
EXPECT=<<EOF
int32   8
uint32  8
hex     0x8
octal   010
unit    8
segment 0000:0008
string  "\b"
fvalue: 8.0
float:  0.000000f
double: 0.000000
binary  0b00001000
trits   0t22
0x8
int32   0
uint32  0
hex     0x0
octal   00
unit    0
segment 0000:0000
string  "\0"
fvalue: 0.0
float:  0.000000f
double: 0.000000
binary  0b00000000
trits   0t0
0x0
EOF
RUN

