NAME=p-h out of bounds
FILE=bins/mach0/mach0-x86-64
CMDS=<<EOF
aaa
p-h @0x100001010 @e:search.in=bin.section
EOF
EXPECT=<<EOF

.-------------------------------------------------------.
| offset       | flags  | funcs  | cmts  | syms  | str  |
)-------------------------------------------------------(
| 0x100001000  | 3      | 0      | 1     | 0     | 0    |
| 0x100001010  | 2      | 0      | 1     | 0     | 0    |
)-------------------------------------------------------(
|              | 5      | 0      | 2     | 0     | 0    |
`-------------------------------------------------------'

EOF
RUN
