CMDS=<<EOF
w helloworld
woE xor s:key
woD xor s:key
ps
EOF
EXPECT=<<EOF
helloworld
EOF
RUN

NAME=woE woD xor fix value
FILE=-
CMDS=<<EOF
b 20
woE xor s:helloworld
ps
EOF
EXPECT=<<EOF
helloworldhelloworld
EOF
RUN

