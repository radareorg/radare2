NAME=pif invalid instruction
FILE=bins/elf/mentalminer
ARGS=-A -B 0x555555554000
BROKEN=1
CMDS=<<EOF
e asm.functions = false
e asm.flags = false
e asm.comments = false
e asm.calls = false
aaa
s sym.parse__IO_FILE__
pif~?
EOF
EXPECT=<<EOF
1367
EOF
RUN

NAME=pif invalid instruction
FILE=bins/elf/mentalminer
ARGS=-A -B 0x555555554000
CMDS=<<EOF
e asm.functions = false
e asm.flags = false
e asm.comments = false
e asm.calls = false
aaa
pif~?
EOF
EXPECT=<<EOF
11
EOF
RUN
