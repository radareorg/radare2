NAME=ooi
FILE=bins/elf/analysis/main
CMDS=<<EOF
e bin.baddr=0
e asm.cmt.calls=false
ooi
op 3
e io.va=0
s entry0
pi 1
?p
EOF
EXPECT=<<EOF
xor ebp, ebp
0x00000410
EOF
RUN

NAME=ooiooi
FILE=bins/elf/analysis/main
CMDS=<<EOF
?e works
ooi;ooi
EOF
EXPECT=<<EOF
works
EOF
RUN

