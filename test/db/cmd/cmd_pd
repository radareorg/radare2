|           0x004028e0      mov esi, str.usr_share_locale              ; 0x414fbf ; "/usr/share/locale"
|           0x00402abd      mov edi, str.TABSIZE                       ; 0x414fe7
--5--
|           0x004028e0      mov esi, str.usr_share_locale              ; 0x414fbf
|           0x00402abd      mov edi, str.TABSIZE                       ; 0x414fe7
EOF
RUN

