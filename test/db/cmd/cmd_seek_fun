NAME=seek next / previous functions
FILE=bins/mach0/mac-ls
ARGS=-a x86 -A
CMDS=<<EOF
s
sn
s
sn
s
sp
s
sp
s
EOF
EXPECT=<<EOF
0x100001058
0x10000116e
0x100001176
0x10000116e
0x100001058
EOF
RUN

NAME=sf function name
FILE=bins/mach0/mac-ls
ARGS=-a x86 -A
CMDS=<<EOF
sf main
s
EOF
EXPECT=<<EOF
0x100001058
EOF
RUN

NAME=sf alias
FILE=bins/mach0/mac-ls
ARGS=-a x86 -A
CMDS=<<EOF
s 0x100001058
sf.
s
EOF
EXPECT=<<EOF
0x100001058
EOF
RUN

NAME=sf partial address
FILE=bins/mach0/mac-ls
ARGS=-a x86 -A
CMDS=<<EOF
sf..1058
s
EOF
EXPECT=<<EOF
0x100001058
EOF
RUN
