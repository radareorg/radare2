# e bin.str.purge=all,!4195920
# izzj
# EOF
# EXPECT=<<EOF
# [{"vaddr":4195920,"paddr":1616,"ordinal":13,"size":44,"length":10,"section":".rodata","type":"utf32le","string":"abcdefêçà  g","blocks":["Basic Latin","Gothic"]}]
