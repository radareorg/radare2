NAME="m68k: m68k filter"
CMDS=<<EOF
e asm.arch=m68k
e asm.bits=32
wx 33fc010000a10000
pi 1
f foo.bar = 0xa10000
pi 1
EOF
EXPECT=<<EOF
move.w 0x100, 0xa10000.l
move.w 0x100, foo.bar
EOF
RUN

NAME="m68k: m68k filter2"
CMDS=<<EOF
e asm.arch=m68k
e asm.bits=32
wx 33fc010000a10000
pi 1
f foo.bar = 0x100
pi 1
EOF
EXPECT=<<EOF
move.w 0x100, 0xa10000.l
move.w foo.bar, 0xa10000.l
EOF
RUN