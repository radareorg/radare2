NAME=wvg decimal value
FILE=-
CMDS=<<EOF
wvg 3.14159
pf F
EOF
EXPECT=<<EOF
0x00000000 = 3.1415899999999999
EOF
RUN

NAME=wvg negative number
FILE=-
CMDS=<<EOF
wvg -123.45
pf F
EOF
EXPECT=<<EOF
0x00000000 = -123.45
EOF
RUN

NAME=wvg positive integer
FILE=-
CMDS=<<EOF
wvg 42
pf F
EOF
EXPECT=<<EOF
0x00000000 = 42
EOF
RUN
