NAME=f_123
FILE=-
ARGS=-1
CMDS=<<EOF
f _foo=123
f _123=_foo
f :123=321
f 123=456
f $123=456
f $foo=456
?v _foo
?v _123
?v :123
?v 123
?v $foo
?v $foo
EOF
EXPECT=<<EOF
ERROR: Invalid flag name '123'
ERROR: Invalid flag name '$123'
ERROR: Invalid flag name '$foo'
0x7b
0x7b
0x141
0x7b
ERROR: cant find flag
0x0
ERROR: cant find flag
0x0
EOF
EXPECT_ERR=
RUN

NAME=f?flag
FILE=-
CMDS=<<EOF
f foo=123
f?foo;?? ?e hello
f?bar;?? ?e world
EOF
EXPECT=<<EOF
hello
EOF
RUN

