NAME=anal fcn names with symbols
FILE=bins/mach0/libr_bp.dylib
ARGS=-A
CMDS=afl~00760
EXPECT=<<EOF
0x00000760    7 379          sym._r_bp_new
EOF
RUN

NAME=anal single fcn
FILE=bins/mach0/libr_bp.dylib
CMDS=<<EOF
s 0x760
af
afi~name
EOF
EXPECT=<<EOF
name: sym._r_bp_new
EOF
RUN

NAME=anal main fcn
FILE=bins/mach0/ls-osx-x86_64
CMDS=<<EOF
af
afi~name
EOF
EXPECT=<<EOF
name: main
EOF
RUN
