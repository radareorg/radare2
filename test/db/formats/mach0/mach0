NAME=mach0 entrypoint
FILE=bins/mach0/hello-osx-i386
CMDS=ie~addr
EXPECT=<<EOF
vaddr=0x00001f14 paddr=0x00000f14 haddr=0x00000464 type=program
EOF
RUN

NAME=mach0 entry0 data
FILE=bins/mach0/hello-osx-i386
CMDS=p8 10 @ entry0
EXPECT=<<EOF
6a0089e583e4f083ec10
EOF
RUN

