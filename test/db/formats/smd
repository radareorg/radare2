NAME=sega megadrive rom entrypoint
FILE=bins/smd/LiquidSpaceDodgerV3.bin
CMDS=ie~vadd
EXPECT=<<EOF
vaddr=0x00000200 paddr=0x00000200 haddr=-1 type=program
EOF
RUN

NAME=sega megadrive rom maps and printing
FILE=bins/smd/LiquidSpaceDodgerV3.bin
CMDS=pxw 4 @ 0x10100
EXPECT=<<EOF
0x00010100  0x00537413                                   .St.
EOF
RUN

NAME=smd strings
