NAME=redirection test
FILE=bins/pdb/Project1.pdb
CMDS=!rabin2 -Pr ${R2_FILE} | grep -ao _unnamed_tag_ | head -n1
EXPECT=<<EOF
_unnamed_tag_
EOF
RUN

