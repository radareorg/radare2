NAME=member1 offset of structure R2_TEST_STRUCT
FILE=bins/pdb/Project1.pdb
CMDS=!rabin2 -P ${R2_FILE} | grep r2_struct_var_1 | grep -ao 0x0
EXPECT=<<EOF
0x0
EOF
RUN

