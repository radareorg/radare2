NAME=VSF: Rambo II- open
FILE=bins/vsf/c64-rambo2-norom.vsf
ARGS=-A
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=VSF: Rambo II- detection
FILE=bins/vsf/c64-rambo2-norom.vsf
CMDS=iI~os
EXPECT=<<EOF
os       C64
EOF
RUN

NAME=VSF: Rambo II- ROMs
FILE=bins/vsf/c64-rambo2-norom.vsf
CMDS=om
EXPECT=<<EOF
* 1 fd: 3 +0x00000084 0x00000000 - 0x0000ffff r-x fmap.RAM
EOF
RUN
