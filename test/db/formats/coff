NAME=tiny coff2
FILE=bins/coff/coff2.obj
CMDS=<<EOF
om
is~text
s sym.__1FooBar__QAE_XZ
pd 2
EOF
EXPECT=<<EOF
10 fd: 3 +0x00000000 0x00000000 - 0x000012f7 r-x 
 9 fd: 3 +0x0000017c 0x0000017c - 0x0000026a --- fmap..drectve_0
