NAME=tiny coff2
FILE=bins/coff/coff2.obj
CMDS=<<EOF
om
is~text
s sym.__1FooBar__QAE_XZ
pd 2
EOF
EXPECT=<<EOF
 9 fd: 3 +0x0000017c 0x00000000 - 0x000000ee --- fmap..drectve_0
 8 fd: 3 +0x0000026b 0x000000f0 - 0x00000b9f r-- fmap..debug_S_1
