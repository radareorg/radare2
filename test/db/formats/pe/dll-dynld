NAME=PE: corkami dll-dynld.exe - open
FILE=bins/pe/dll-dynld.exe
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami dll-dynld.exe - entrypoint
FILE=bins/pe/dll-dynld.exe
CMDS=s
EXPECT=<<EOF
0x401000
EOF
RUN

NAME=PE: corkami dll-dynld.exe - pi 1
FILE=bins/pe/dll-dynld.exe
CMDS=<<EOF
e asm.filter=false
pi 1
EOF
EXPECT=<<EOF
push 0x401064
EOF
RUN
