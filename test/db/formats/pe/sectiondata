RUN

NAME=PE: corkami cm01.exe - section refs 1
FILE=bins/pe/cm01.exe
CMDS=<<EOF
e anal.strings = true
aaa
pi 2 @ 0x004010b7
EOF
EXPECT=<<EOF
push str.CrackMe_v1.0
push str.No_need_to_disasm_the_code_
EOF
RUN

