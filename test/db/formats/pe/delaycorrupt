NAME=PE: corkami delaycorrupt.exe - open
FILE=bins/pe/delaycorrupt.exe
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami delaycorrupt.exe - entrypoint
FILE=bins/pe/delaycorrupt.exe
CMDS=s
EXPECT=<<EOF
0x401000
EOF
RUN

NAME=PE: corkami delaycorrupt.exe - pi 1
FILE=bins/pe/delaycorrupt.exe
CMDS=<<EOF
e asm.filter=false
pi 1
EOF
EXPECT=<<EOF
push 0x401018
EOF
RUN
