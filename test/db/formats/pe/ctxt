NAME=PE: corkami ctxt.dll - open
FILE=bins/pe/ctxt.dll
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami ctxt.dll - entrypoint
FILE=bins/pe/ctxt.dll
CMDS=s
EXPECT=<<EOF
0x1001000
EOF
RUN

NAME=PE: corkami ctxt.dll - entrypoint
FILE=bins/pe/ctxt.dll
CMDS=s
EXPECT=<<EOF
0x1001000
EOF
RUN

NAME=PE: corkami ctxt.dll - pi 1
FILE=bins/pe/ctxt.dll
CMDS=<<EOF
e asm.filter=false
pi 1
EOF
EXPECT=<<EOF
cmp dword [esp + 8], 1
EOF
RUN
