NAME=obj entry
FILE=bins/elf/analysis/test.obj
BROKEN=1
CMDS=ie~addr
EXPECT=<<EOF
vaddr=0x00000000 paddr=0x00000040 baddr=0x00000000 laddr=0x00000000 haddr=0x00000018 type=program
EOF
RUN

NAME=obj entry
FILE=bins/elf/analysis/test.obj
BROKEN=1
CMDS=ie~addr
EXPECT=<<EOF
vaddr=0x00200000 paddr=0x00000040 baddr=0x00200000 laddr=0x00000000 haddr=0x00000018 type=program
EOF
RUN

NAME=obj main address
FILE=bins/elf/analysis/test.obj
BROKEN=1
CMDS=?v sym.main
EXPECT=<<EOF
0x10
EOF
RUN

NAME=obj main address
FILE=bins/elf/analysis/test.obj
BROKEN=1
CMDS=?v sym.main
EXPECT=<<EOF
0x200010
EOF
RUN

NAME=obj main
FILE=bins/elf/analysis/test.obj
BROKEN=1
CMDS=pi 3@ sym.main
EXPECT=<<EOF
push rbp
mov rbp, rsp
call 0x19
EOF
RUN

NAME=obj main baddr
FILE=bins/elf/analysis/test.obj
BROKEN=1
CMDS=pi 3@sym.main
EXPECT=<<EOF
push rbp
mov rbp, rsp
call 0x200019
EOF
RUN
