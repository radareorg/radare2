NAME=random39855 relocs
FILE=bins/elf/random_39855/random_39855.oo
ARGS=-e bin.relocs.apply=true
CMDS=<<EOF
e bin.relocs.apply=true
s sym.func_9
e asm.var.summary=0
e emu.str=true
af
pd 20
EOF
EXPECT=<<EOF
