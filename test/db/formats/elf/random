NAME=random39855 relocs bin
FILE=bins/elf/random_39855/random_39855
CMDS=<<EOF
e asm.var.summary=0
e emu.str=true
s sym.func_9
af
pd 20
pxq 8 @ str.O_n_
EOF
EXPECT=<<EOF
