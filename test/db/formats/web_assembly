NAME=WASM: Wasm - open
FILE=bins/wasm/basic.wasm
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=WASM: Wasm - dotnet open
FILE=bins/wasm/dotnet.wasm
CMDS=<<EOF
i
EOF
EXPECT=<<EOF
