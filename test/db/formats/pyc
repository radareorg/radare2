NAME=pyc load version39
FILE=bins/pyc/py39.pyc
CMDS=<<EOF
iI~machine
EOF
EXPECT=<<EOF
machine  Python v3.9.0 VM (rev 123)
EOF
RUN

NAME=pyc load version38
FILE=bins/pyc/py38.pyc
CMDS=<<EOF
dr
dr=
EOF
EXPECT=<<EOF
     a0 0x00000000            a1 0x00000000            a2 0x00000000
     a3 0x00000000            r0 0x00000000            sp 0x00000000
     pc 0x00000000            bp 0x00000000       
EOF
EXPECT_ERR=
RUN

