NAME=adf analysis on an obfuscated executable
FILE=bins/pe/cmd_adf_sample0.exe
BROKEN=1
CMDS=<<EOF
e asm.arch=x86
e asm.bits=32
e asm.os=linux
e asm.lines.jmp=false
e asm.lines.out=false
e asm.bytes=false
e asm.indentspace=0
e asm.jmphints=false
e asm.xrefs=false
e asm.functions=false
e asm.fcncalls=false
e asm.lines.fcn=false
e scr.utf8=false
e anal.jmp.after=false
e anal.calls=false
e anal.jmp.cref=false
e anal.jmp.above=true
e anal.jmp.ref=true
e anal.split=true
# HACK to avoid ASAN taking too long on travis.. this is broken anyway
e anal.bb.maxsize=1K
adf @ sym.testObf27.exe_VirtMe
.adf @ sym.testObf27.exe_VirtMe
adf @ 0x00560e67
.adf @ 0x00560e67
pd 4 @ 0x00560e67
EOF
