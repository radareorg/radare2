# somehow the CI cant disasm with those plugins, while it runs in local and the plugin is there
NAME=v850.np r0ref
BROKEN=1
FILE=-
CMDS=<<EOF
e anal.arch=?~v850
e asm.bits=32
e asm.arch=v850.np
wx 830c
pd 1
wx e00f71f0
f mmio.ICC = 0xfffff070
pd 1
EOF
EXPECT=<<EOF
            0x00000000      830c           sst.h r1,  6[ep]
            0x00000000      e00f71f0       ld.hu mmio.ICC,  r1         ; 0xfffff070
EOF
RUN
