NAME=add reg, imm
FILE=malloc://128
CMDS=<<EOF
e asm.arch=bpf
e asm.bits=32
wx 0400000001000000
ao@0
EOF
EXPECT=<<EOF
address: 0x0
opcode: add 0x1
esilcost: 0
disasm: add 0x1
pseudo: 0x1 += 
mnemonic: add
description: adds src to dst (32-bit)
mask: ffffffffffffffff
prefix: 0
id: 1
bytes: 0400000001000000
val: 0x00000001
refptr: 0
size: 8
sign: false
type: add
cycles: 0
esil: 1,a,+=
family: cpu
EOF
RUN
