NAME=rcall
FILE=-
CMDS=<<EOF
e asm.arch=arm
e asm.bits=64
wx e18f1394 # BL XXX
s+4
wx 00013fd6 # BLR x8
ao~type[1]
ao@0~type[1]
/at call
?e --
/at ucall
?e --
/at rcall
EOF
EXPECT=<<EOF
rcall
call
0x00000000 4 bl 0x4e3f84
--
--
0x00000004 4 blr x8
EOF
RUN

NAME=noreturn errno
FILE=bins/mach0/BatteryLife.arm_64.1
CMDS=<<EOF
e emu.str=1
s 0x100011130
pd 2~addChild
EOF
EXPECT=<<EOF
EOF
RUN

NAME=adrp string
FILE=bins/mach0/SwiftUITest
CMDS=<<EOF
e emu.str=1
s..886c
pds 10
EOF
EXPECT=<<EOF
0x100008870 str.Hello_World
EOF
RUN

NAME=symbol with invalid offset
FILE=bins/elf/libarm64.so
CMDS=<<EOF
ii
iE
EOF
EXPECT=<<EOF
[Imports]
nth vaddr      bind   type   lib name
-------------------------------------
3   0x00020060 GLOBAL NOTYPE     _bss_end__
4   0x000006b0 GLOBAL FUNC       realloc
