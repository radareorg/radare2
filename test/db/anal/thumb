NAME=thumb tbb jmptbl
FILE=malloc://512
CMDS=<<EOF
e asm.arch=arm
e asm.bits=16
e asm.nbytes=6
e asm.flags.inbytes=true
wx 
af
pdr~case

e asm.arch=arm
e asm.bits=16
e asm.nbytes=32
e asm.flags.inbytes=true
wx 
af
pdr~case
EOF
EXPECT=<<EOF
| 0x00000008                    bhs case.0xa.0
| 0x0000000a      switch.0x0.:  tbb [0x00000010]                       ; switch table (6 cases) at 0xe
| 0x00000014       case.0xa.2:  ldr r0, [0x00000050]                   ; [0x50:4]=0x200001a1
| 0x0000001a                    beq case.0xa.0
| 0x0000002a       case.0xa.3:  movs r0, 0
| 0x00000038       case.0xa.5:  movs r0, 0
\ 0x00000048      case.0xa.0.:  pop {r4, pc}
| 0x00000008                                                                        bhs case.0xa.0
| 0x0000000a                                                    switch.0x0000000a:  tbb [0x00000010] ; switch table (6 cases) at 0xe
| 0x00000014                                                           case.0xa.2:  ldr r0, [0x00000050] ; [0x50:4]=0x200001a1
| 0x0000001a                                                                        beq case.0xa.0
| 0x0000002a                                                           case.0xa.3:  movs r0, 0
| 0x00000038                                                           case.0xa.5:  movs r0, 0
\ 0x00000048                                     case.0xa.0,case.0xa.1,case.0xa.4:  pop {r4, pc}
EOF
RUN

NAME=thumb unaligned mov, pc
FILE=malloc://512
CMDS=<<EOF
e asm.arch=arm
e asm.bits=16
wx 01308fe213ff2fe178460c30c0460190491a921a0b2701df2f62696e2f736800
s8
af
aeim
aepc 8
2ds
dr?r0
x 16 @r:r0
EOF
EXPECT=<<EOF
0x00000018
- offset -  1819 1A1B 1C1D 1E1F 2021 2223 2425 2627  89ABCDEF01234567
0x00000018  2f62 696e 2f73 6800 0000 0000 0000 0000  /bin/sh.........
EOF
RUN

NAME=false posititve syscalls in thumb code
FILE=bins/elf/libarm.so
CMDS=<<EOF
aae
pi 1 @@ syscall.*
EOF
EXPECT=<<EOF
svc 0xef
svc 0x9e
svc 0x72
svc 0xa7
svc 0x42
svc 0xb6
EOF
RUN

NAME=arm/thumb imports
FILE=bins/elf/libmagic.so
CMDS=<<EOF
pd 10
?en ahb32=
ahb*~b 32?
?en ahb16=
ahb*~b 16?
0x00005e34
ahb.
-b asm.lines=0
pd 4
EOF
EXPECT=<<EOF
        :   ;-- section..text:
        :   ;-- entry0:
        :   ;-- entry.fini0:
        :   0x00005fa8      04009fe5       ldr r0, [0x00005fb4]        ; [0x5fb4:4]=0x1104c ; [09] -r-x section size 53128 named .text
        :   0x00005fac      00008fe0       add r0, pc, r0
        `=< 0x00005fb0      93ffffea       b sym.imp.__cxa_finalize
            0x00005fb4      4c100100       andeq r1, r1, ip, asr 32
            ;-- entry.init0:
            0x00005fb8      10402de9       push {r4, lr}
            0x00005fbc      40409fe5       ldr r4, [0x00006004]        ; [0x6004:4]=0x11064
            0x00005fc0      40109fe5       ldr r1, [0x00006008]        ; [0x6008:4]=0x29e0 "xxabiv115__forced_unwindD2Ev"
            0x00005fc4      04408fe0       add r4, pc, r4
            0x00005fc8      0030a0e3       mov r3, 0
            0x00005fcc      0400a0e1       mov r0, r4
ahb32=323
ahb16=65
32
            ;-- rsym.strdup:
            ;-- strdup:
            0x00005e34      00c68fe2       add ip, pc, 0, 12
            0x00005e38      11ca8ce2       add ip, ip, 0x11000
            0x00005e3c      48f1bce5       ldr pc, [ip, 0x148]!
            ;-- rsym.strstr:
            ;-- strstr:
            0x00005e40      00c68fe2       add ip, pc, 0, 12
EOF
RUN
