NAME=afbo
FILE=bins/mach0/mac-ls2
CMDS=<<EOF
af
abo~?
?e --
abo~?
afbo~?
"(nops;f opcount=opcount+`abo~?`)"
.(nops)@@=`afbq`
afi~nins
?vi opcount
EOF
EXPECT=<<EOF
14
--
14
421
421
EOF
RUN
