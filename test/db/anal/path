FILE=bins/mach0/mac-ls2
CMDS=<<EOF
aaa
abp 0x10000168b @ 0x1000011e8
EOF
EXPECT=<<EOF
0x1000011e8
0x100001216
0x100001290
0x1000012b6
0x1000012c6
0x1000012e7
0x100001682
0x10000168b
EOF
RUN

