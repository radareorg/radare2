NAME=pd 10 align
FILE=bins/s390/zos/prueba/prueba
CMDS=<<EOF
/x 47F0F024
s hit0_0
e asm.arch=s390
pd 10
e asm.arch=s390.gnu
pd 10
EOF
EXPECT=<<EOF
