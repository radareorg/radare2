NAME=aav find xrefs
FILE=bins/mach0/macho-bash
CMDS=<<EOF
e anal.loads=true
e anal.in=range
e anal.from=0x8000
e anal.to=0x8000+4K
aac
afna
aar
aan
aav
# aa;afna;aac;aar;aan;aav
pd 17 @0x8058
EOF
EXPECT=<<EOF
|       `=< 0x00008058      03e4           b 0x7862
            0x0000805a      0000           movs r0, r0
            ; DATA XREF from sub.free_73a0 @ 0x7d30
