mask: ffffffff
prefix: 0
id: 424
bytes: 08402de9
refptr: 0
size: 4
sign: false
type: push
cycles: 1
esil: 8,sp,-=,lr,r3,2,sp,=[*]
family: cpu
stackop: inc
stackptr: 8
--
address: 0x50e
