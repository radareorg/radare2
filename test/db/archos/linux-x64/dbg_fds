NAME=dbg.fds.count
FILE=bins/elf/analysis/elf-nx
ARGS=-d
CMDS=<<EOF
dd~?
dk 9
EOF
EXPECT=<<EOF
3
EOF
RUN

NAME=dbg.fds.close
FILE=bins/elf/analysis/elf-nx
ARGS=-d
BROKEN=1
CMDS=<<EOF
dd~?
dd-1
dd~?
dk 9
EOF
EXPECT=<<EOF
3
2
EOF
RUN

NAME=dbg.fds.count
FILE=bins/elf/analysis/elf-nx
ARGS=-d
CMDS=<<EOF
dd~?
dk 9
EOF
EXPECT=<<EOF
3
EOF
RUN


