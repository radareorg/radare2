NAME=di on SIGABRT
BROKEN=1
FILE=bins/elf/signals/signal-input
ARGS=-e cfg.fortunes=false -e dbg.args=6 -d
CMDS=<<EOF
dc
di~type
di~^signal
di~^signum
di~^sigstr
di~stopreason
EOF
EXPECT=<<EOF
[+] signal 6 aka SIGABRT received 0 (Abort)
type=signal
signal=SIGABRT
signum=6
sigstr=Abort
stopreason=stopped
Hello, World!
EOF
RUN


