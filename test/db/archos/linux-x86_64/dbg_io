NAME=dbg.readpc
FILE=bins/elf/ls-focal
ARGS=-d
CMDS=<<EOF
sr PC
p8 4
dk 9
NOT_EXPECT=1
EOF
EXPECT=<<EOF
4889e7e8
EOF
RUN

NAME=dbg.read.entry0
FILE=bins/elf/analysis/elf-nx
ARGS=-d
CMDS=<<EOF
s entry0
p8 4
EOF
EXPECT=<<EOF
31ed5e89
EOF
RUN

NAME=dbg.write
FILE=bins/elf/ls-focal
ARGS=-d
CMDS=<<EOF
s entry0
p8 2
wx 9090
p8 2
dk 9
EOF
EXPECT=<<EOF
f30f
9090
EOF
RUN
