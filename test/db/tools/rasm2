EXPECT=<<EOF
9083e803bb00000000cd80c3
EOF
RUN

NAME=rasm2  SPP nested conditionals
FILE=-
CMDS=!rasm2 -a x86 -b 32 -p -f bins/other/rasm2/asm/nest.asm
EXPECT=<<EOF
83c10283e90583c301b90200000090
EOF
EXPECT_ERR=<<EOF
WARNING: WARN (line=1)
ERROR: ERR (line=1)
EOF
RUN

