!rasm2 -a x86 -b 64 "mov eax, 0x1122334455"
EOF
EXPECT=
EXPECT_ERR=<<EOF
ERROR: Cannot assemble 'mov rax, 0x112233445566778899' at line 1
ERROR: Cannot assemble 'mov eax, 0x1122334455' at line 1
EOF
RUN

