NAME=baddr ==
FILE=bins/elf/analysis/hello-linux-x86_64
CMDS=!rabin2 -qe ${R2_FILE};!rabin2 -B 0x400000 -qe ${R2_FILE}
EXPECT=<<EOF
0x00400410
0x00400410
EOF
RUN

