EOF
RUN

NAME=cmp flags
FILE=malloc://0x200
CMDS=<<EOF
e asm.arch=arm
e asm.bits=64
wx 1ffc0371 # cmp w0, 0xff
aer w0=0x55
?v pstate & 0xf0000000
aes
?v pstate & 0xf0000000
aer nf
aer zf
aer cf
aer vf
EOF
EXPECT=<<EOF
0x0
0x80000000
0x00000001
0x00000000
0x00000000
0x00000000
EOF
RUN

NAME=cbz/cbnz likeliness
FILE=malloc://0x200
CMDS=<<EOF
e asm.arch=arm
e asm.bits=64
e cfg.bigendian=false
e emu.str=true
wv 0x52800015
wv 0x34000035 @ 4
wv 0x52800035 @ 8
wv 0x34000035 @ 12
wv 0x52800015 @ 16
wv 0x35000035 @ 20
wv 0x52800035 @ 24
wv 0x35000035 @ 28
pd 8
EOF
EXPECT=<<EOF
            0x00000000      15008052       movz w21, 0
        ,=< 0x00000004      35000034       cbz w21, 8                  ; likely
        `-> 0x00000008      35008052       movz w21, 0x1
        ,=< 0x0000000c      35000034       cbz w21, 0x10               ; unlikely
        `-> 0x00000010      15008052       movz w21, 0
        ,=< 0x00000014      35000035       cbnz w21, 0x18              ; unlikely
        `-> 0x00000018      35008052       movz w21, 0x1
        ,=< 0x0000001c      35000035       cbnz w21, 0x20              ; likely
EOF
RUN

NAME=bic 64-bit register
FILE=malloc://0x200
CMDS=<<EOF
e asm.arch=arm
e asm.bits=64
wx 4a012b8a #bic x10, x11
aer x10=0x1122334455667788
aer x11=0xff
aes
aer x10
EOF
EXPECT=<<EOF
0x1122334455667700
EOF
RUN

