NAME=64bit Integer comparison [-1 == 1 (op1 < op2)]
FILE=-
CMDS=<<EOF
e asm.arch = x86
e asm.bits = 64
"ae 1,-1,==,63,$o,of,:=,63,$s,sf,:=,64,$b,cf,:=,$z,zf,:="
"aer?cf"
"aer?sf"
"aer?of"
"aer?zf"
EOF
EXPECT=<<EOF
0x00000000
0x00000001
0x00000000
0x00000000
EOF
RUN

NAME=64bit Integer comparison [1 == -1 (op1 > op2)]
FILE=-
CMDS=<<EOF
e asm.arch = x86
e asm.bits = 64
"ae -1,1,==,63,$o,of,:=,63,$s,sf,:=,64,$b,cf,:=,$z,zf,:="
"aer?cf"
"aer?sf"
"aer?of"
"aer?zf"
EOF
EXPECT=<<EOF
0x00000001
0x00000000
0x00000000
0x00000000
EOF
RUN

NAME=32bit Integer addition with overflow [0x80000000 + 0x80000000]
FILE=-
CMDS=<<EOF
e asm.arch = x86
e asm.bits = 64
"ae 0x80000000,eax,:=,0x80000000,ebx,:=,eax,ebx,+=,31,$o,of,:=,31,$s,sf,:=,31,$c,cf,:=,$z,zf,:="
"aer?cf"
"aer?sf"
"aer?of"
"aer?zf"
EOF
EXPECT=<<EOF
0x00000001
0x00000000
0x00000001
0x00000001
EOF
RUN

NAME=32bit Integer Addition [0x90000000 + 0x70000000]
