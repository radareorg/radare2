NAME=negative mem reference
FILE=malloc://0x200
ARGS=-m 0x410984
CMDS=<<EOF
e io.va=true
e asm.arch=mips
e asm.bits=32
e cfg.bigendian=true
aei
aeim
aeip
ar > /dev/null
ar t9=0
ar gp=0
wx 3c1c0042279c89408f99805011223344
aes
aes
aes
ar t9
EOF
EXPECT=<<EOF
0x11223344
EOF
RUN

NAME=complex function
FILE=malloc://0x800
ARGS=-m 0x0040092c
CMDS=<<EOF
e io.va=true
e asm.arch=mips
e asm.bits=32
ar > /dev/null
ar t9=0
ar v0=14
ar a0=0x00400a7c
ar fp=0x00401000
wx 
wx 7c0a4000 @ 0x00401030
wx 0d000000 @ 0x00401020
aeip
aesu 0x00400a74
s 0x00400a7c
psz
EOF
EXPECT=<<EOF
SLOWDOWNWOMAN
EOF
RUN

