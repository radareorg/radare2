NAME=asm.emu with meta
FILE=bins/elf/analysis/crackmips
CMDS=<<EOF
s 0x00400820
af
e asm.emu=true
e emu.write=true
e anal.gpfixed=false
e asm.lines.bb=false
e asm.lines.fcn=false
e asm.imm.str=false
aei
aeim
pdf~0x00400828[7]
EOF
EXPECT=<<EOF
0x415424
EOF
RUN

