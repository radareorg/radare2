NAME=2step push x86-32
FILE=-
CMDS=<<EOF
e io.va=true
e esil.debug=true
e asm.arch=x86
e asm.bits=32
aei
aeim
aer esp=0x00108000
.aer*
"wa mov eax, 0x1234;push eax"
2aes
.aer*
aer?esp
?v [esp]
EOF
EXPECT=<<EOF
0x00107ffc
0x1234
EOF
RUN

