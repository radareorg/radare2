NAME=thumb emulation arm issue
FILE=bins/mach0/FileDP
CMDS=<<EOF
aes
e asm.bits
EOF
EXPECT=<<EOF
32
EOF
RUN

NAME=string refs: thumb
FILE=bins/mach0/test-thumb
CMDS=<<EOF
af
e asm.emu=true
e emu.str=true
pdf~?str.string
EOF
EXPECT=<<EOF
6
EOF
RUN

