NAME=ldm r3!, {r1, r4, r5}
FILE=malloc://0x200
BROKEN=1
CMDS=<<EOF
e asm.arch=arm
e asm.bits=16
ar > /dev/null
ar r3=4		# address
ar r1=0		# init with dummy values
ar r4=0
ar r5=0
wx 32cb
wx 111111114444444455555555@4
aes
ar r1; ar r4; ar r5; ar r3
EOF
EXPECT=<<EOF
0x11111111
0x44444444
0x55555555
0x00000010
EOF
RUN

