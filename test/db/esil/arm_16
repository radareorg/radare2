NAME=Pre-indexed addressing mode
FILE=malloc://0x200
CMDS=<<EOF
e asm.arch=arm
e asm.bits=16
wx 10f8013f # ldrb r3, [r0, 1]!
aes
aer pc
aer r0
aer r3
EOF
EXPECT=<<EOF
0x00000004
0x00000001
0x000000f8
EOF
RUN
