NAME=cmp 1,2,<
FILE=-
CMDS="ae 1,2,<"
EXPECT=<<EOF
0
EOF
RUN

NAME=cmp 2,1,<
FILE=-
CMDS="ae 2,1,<"
EXPECT=<<EOF
0x1
EOF
RUN

NAME=cmp 1,2,>
FILE=-
CMDS="ae 1,2,>"
EXPECT=<<EOF
0x1
EOF
RUN

NAME=cmp 2,1,>
FILE=-
CMDS="ae 2,1,>"
EXPECT=<<EOF
0
EOF
RUN

NAME=cmp 1,2,<=
FILE=-
CMDS="ae 1,2,<="
EXPECT=<<EOF
0
EOF
RUN

NAME=cmp 2,1,<=
FILE=-
CMDS="ae 2,1,<="
EXPECT=<<EOF
0x1
EOF
RUN

NAME=cmp 1,2,>=
FILE=-
CMDS="ae 1,2,>="
EXPECT=<<EOF
0x1
EOF
RUN

NAME=cmp 2,1,>=
FILE=-
CMDS="ae 2,1,>="
EXPECT=<<EOF
0
EOF
RUN

NAME=cmp 1,1,<=
FILE=-
CMDS="ae 1,1,<="
EXPECT=<<EOF
0x1
EOF
RUN

NAME=cmp 1,1,>=
FILE=-
CMDS="ae 1,1,>="
EXPECT=<<EOF
0x1
EOF
RUN

NAME=cmp 1,1,==
FILE=-
CMDS="ae 1,1,==,$z,NUM"
EXPECT=<<EOF
0x1
EOF
RUN

NAME=cmp 1,2,==
FILE=-
CMDS="ae 1,2,==,$z,NUM"
EXPECT=<<EOF
0
EOF
RUN

NAME=cmp -1,1,>
FILE=-
CMDS="ae -1,1,>"
EXPECT=<<EOF
0x1
EOF
RUN

