R2PM_BEGIN

R2PM_GIT "https://github.com/mrmacete/r2scripts"
R2PM_DESC "[r2-r2pipe-python] Burn asm.emu results in real comments (MIPS oriented)"

R2PM_INSTALL() {
	mkdir -p "${R2_RCD}" || exit 1
	cd esilburner || exit 1
	echo '$esilburner=#!pipe python '"'`pwd`'"'/esilburner.py' > "${R2_RCD}/esilburner" || exit 1
}

R2PM_UNINSTALL() {
	rm -f "${R2_RCD}"/esilburner
}

R2PM_END
